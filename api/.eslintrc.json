{
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["prettier", "eslint:recommended", "plugin:node/recommended"],
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error", "info"] }],
    "no-use-before-define": "warn",
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "spaced-comment": "warn"
  }
}
